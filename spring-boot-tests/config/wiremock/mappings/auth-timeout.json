{
  "request": {
    "method": "POST",
    "urlPath": "/auth",
    "headers": {
      "Content-Type": {
        "equalTo": "application/x-www-form-urlencoded"
      },
      "Accept": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "matches": "token=TIMEOUT.*"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "fixedDelayMilliseconds": 10000,
    "body": "{\"status\": \"timeout\"}"
  },
  "priority": 3
}
