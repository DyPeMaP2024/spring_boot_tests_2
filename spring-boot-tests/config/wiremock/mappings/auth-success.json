{
  "request": {
    "method": "POST",
    "urlPath": "/auth",
    "headers": {
      "Content-Type": {
        "equalTo": "application/x-www-form-urlencoded"
      },
      "Accept": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "matches": "token=[0-9A-F]{32}"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "body": "{\"status\": \"authenticated\"}"
  },
  "priority": 1
}
