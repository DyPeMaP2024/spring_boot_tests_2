{
  "request": {
    "method": "POST",
    "urlPath": "/doAction",
    "headers": {
      "Content-Type": {
        "equalTo": "application/x-www-form-urlencoded"
      },
      "Accept": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "matches": "token=ERROR.*"
      }
    ]
  },
  "response": {
    "status": 500,
    "headers": {
      "Content-Type": "application/json"
    },
    "body": "{\"error\": \"Action failed\"}"
  },
  "priority": 2
}
